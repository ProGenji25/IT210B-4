<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - IT210B_4</title>
    <link rel="stylesheet" href="~/css/materialize.css" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav>
            <div class="nav-wrapper">
                <a class="brand-logo" asp-area="" asp-controller="Items" asp-action="Index">IT210B_4</a>
                <ul class="right hide-on-med-and-down" id="nav-mobile">
                    <li>
                        <a class="waves-effect waves-light btn" asp-area="" asp-controller="Items" asp-action="Index">Home</a>
                    </li>
                    <partial name="_LoginPartial" />
                    <li>
                        <a class="waves-effect waves-light btn" asp-area="" asp-controller="Items" asp-action="Privacy">Privacy</a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - IT210B_4 - <a asp-area="" asp-controller="Items" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/materialize.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
